{
  "name": "fastapp-framework",
  "description": "FastApp Framework Backend",
  "author": "Symbiosika GmbH",
  "license": "proprietary",
  "homepage": "https://github.com/symbiosika/fastapp-framework#readme",
  "version": "0.1.1",
  "main": "./lib/index.js",
  "keywords": [
    "LLM",
    "GPT",
    "Promping",
    "Scripting",
    "AI",
    "Knowledge Database"
  ],
  "scripts": {
    "dev": "bun --hot run demo.ts",
    "dev:inspect": "bun --inspect --hot run demo.ts",
    "migrate": "drizzle-kit migrate",
    "push": "drizzle-kit push",
    "generate": "drizzle-kit generate",
    "studio": "drizzle-kit studio",
    "build": "tsc",
    "clean": "rm -rf ./lib/"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "files": [
    "lib/**/*"
  ],
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./dbSchema": {
      "types": "./lib/dbSchema.d.ts",
      "default": "./lib/dbSchema.js"
    }
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/nodemailer": "^6.4.16",
    "@types/pg": "^8.11.10",
    "@types/turndown": "^5.0.5",
    "drizzle-kit": "^0.26.2",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "cron": "^3.1.7",
    "csv": "^6.3.10",
    "drizzle-orm": "^0.35.3",
    "hono": "^4.6.5",
    "jsonwebtoken": "^9.0.2",
    "magic-prompt": "0.3.5",
    "nanoid": "^5.0.7",
    "nodemailer": "^6.9.15",
    "openai": "^4.40.2",
    "pg": "^8.13.0",
    "pgvector": "^0.1.8",
    "stripe": "^17.1.0",
    "turndown": "^7.2.0",
    "valibot": "^1.0.0-beta.0"
  }
}
